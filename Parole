#include <iostream>
using namespace std;
int main()
{

	int Vlad = 32, Barak = 20;

	if (Vlad > 0 && Barak < 30)
	{

		cout << "Vlads age is ok" << endl;

	}

	else cout << "wrong Vlads age" << endl;

	if (Vlad > 0 && Vlad < 10)

	{

		cout << "Vlads age is ok" << endl; 
	}

	else cout << "wrong Barak age" << endl;

}

